/* pages/my/my.wxss */
.my-top{
  width: 100%;
  height: 200rpx;
  display: flex;
  justify-content:space-around;
  align-items: center; 
  background-color:rgb(255,255,255);
}
.my-top>image{
  width:150rpx;
  height:150rpx;
  border-radius: 20rpx;
}

.list-nav {
  width: 100%;
  height: 100rpx;
}

.lit-nav > view {
  width: 100%;
  display: flex;
  margin-top: 20rpx;
  padding: 20rpx 60rpx;
  background-color: white;
  border-top: 5rpx solid rgb(239, 239, 239);
}
